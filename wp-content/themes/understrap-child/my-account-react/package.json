{
  "name": "samsara-my-account-react",
  "version": "1.0.0",
  "description": "Samsara My Account - React Dashboard for WooCommerce",
  "private": true,
  "scripts": {
    "start": "npx webpack serve --mode development --config webpack.config.js",
    "prebuild": "find src/components src/pages -name '*.js' -type f ! -name 'utils.js' -exec sh -c 'base=\"${1%.js}\"; if [ -f \"${base}.jsx\" ]; then echo \"Removing duplicate: $1\"; rm \"$1\"; fi' _ {} \\;",
    "build": "npx webpack --mode production --config webpack.config.js",
    "build:css": "npx tailwindcss -i ./src/styles/input.css -o ./build/css/my-account.css",
    "build:all": "npm run build && npm run build:css"
  },
  "dependencies": {
    "@babel/core": "^7.28.4",
    "@babel/preset-react": "^7.27.1",
    "@hookform/resolvers": "^5.0.1",
    "@radix-ui/react-accordion": "^1.2.2",
    "@radix-ui/react-alert-dialog": "^1.1.4",
    "@radix-ui/react-avatar": "^1.1.2",
    "@radix-ui/react-checkbox": "^1.1.3",
    "@radix-ui/react-dialog": "^1.1.4",
    "@radix-ui/react-dropdown-menu": "^2.1.4",
    "@radix-ui/react-label": "^2.1.1",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-popover": "^1.1.4",
    "@radix-ui/react-select": "^2.1.4",
    "@radix-ui/react-separator": "^1.1.1",
    "@radix-ui/react-slot": "^1.1.1",
    "@radix-ui/react-tabs": "^1.1.2",
    "@radix-ui/react-toast": "^1.2.4",
    "@wordpress/api-fetch": "^7.0.0",
    "@wordpress/components": "^28.0.0",
    "@wordpress/data": "^10.0.0",
    "@wordpress/element": "^6.0.0",
    "autoprefixer": "^10.4.20",
    "axios": "^1.7.9",
    "babel-loader": "^10.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "css-loader": "^7.1.2",
    "date-fns": "^4.1.0",
    "embla-carousel-react": "^8.6.0",
    "input-otp": "^1.4.2",
    "lucide-react": "^0.468.0",
    "next-themes": "^0.4.6",
    "postcss": "^8.4.49",
    "postcss-loader": "^8.2.0",
    "react": "^18.3.1",
    "react-day-picker": "^9.11.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.56.2",
    "react-resizable-panels": "^3.0.6",
    "react-router-dom": "^7.5.1",
    "sonner": "^1.7.1",
    "style-loader": "^4.0.0",
    "tailwind-merge": "^2.6.0",
    "tailwindcss": "^3.4.18",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "^1.1.2",
    "webpack": "^5.102.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@wordpress/scripts": "^27.9.0"
  }
}
